    <bpmn:sequenceFlow id="SequenceFlow_0dllsek" name="Yes" sourceRef="ExclusiveGateway_12k03m4" targetRef="EndEvent_0q6qume">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" language="JavaScript">this.environment.output.remainder === 0;</bpmn:conditionExpression>
