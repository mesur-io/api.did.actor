     <bpmn:scriptTask id="Task_1ri0x4y" name="Calculate remainder modulo 2" scriptFormat="JavaScript">
     <script>
        const remainder = this.environment.variables.number %2;
        // this.environment.services.console.log("remainder: ",  remainder);
        this.environment.output.remainder = remainder;
        next(null, { remainder });
      </script>
      <bpmn:incoming>SequenceFlow_006qw3j</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1owsevh</bpmn:outgoing>
    </bpmn:scriptTask>
    <bpmn:exclusiveGateway id="ExclusiveGateway_12k03m4" name="Is remainder 0?">
      <bpmn:incoming>SequenceFlow_1owsevh</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_0dllsek</bpmn:outgoing>
      <bpmn:outgoing>SequenceFlow_0ga9ary</bpmn:outgoing>
    </bpmn:exclusiveGateway>
        <bpmn:endEvent id="EndEvent_0q6qume" name="End with an even number">
      <bpmn:incoming>SequenceFlow_0dllsek</bpmn:incoming>
    </bpmn:endEvent>
